<article class='user'>
  <div class='row'>
    <div class='small-12 medium-6 large-4 columns'>
      <h1>Users</h1>
      <ul>
        <% @users.each do |user| %>
          <li>
            <%= link_to user.name, user_path(user) %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</article>