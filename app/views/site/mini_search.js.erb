var miniSearchObject = $("[data-mini-search='<%= @search_type %>']")
miniSearchObject.parent().find(".default-option").hide();
miniSearchObject.parent().find('.search-result').remove();
<% @mini_search_results.reverse.each do |result| %>
  $("li[data-mini-search='<%= @search_type %>'").after("<%= j( render 'site/mini_search', result: result, search_type: @search_type, category: @category ) %>");
<% end %>
